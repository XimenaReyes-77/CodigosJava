import java.util.Scanner;
public class Mayorde3 {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int a, b, c;
        System.out.println("Ingresa 3 números diferentes: ");
        a = scanner.nextInt();
        b = scanner.nextInt();
        c = scanner.nextInt();
        
        if (a != b && a != c && b != c) {
            // Determinar el número mayor usando una estructura simple
            if (a > b && a > c) {
                System.out.println("El número mayor es " + a);
            } else if (b > a && b > c) {
                System.out.println("El número mayor es " + b);
            } else {
                System.out.println("El número mayor es " + c);
            }
        } else {
            System.out.println("Los números deben ser diferentes entre sí");
        }
        scanner.close();
    }
}
