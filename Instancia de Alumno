public class Alumno {
    private String nombre;

    public Alumno(String nombre) {
        this.nombre = nombre;
    }
    @Override
    public String toString() {
        return "Mi texto personalizado " + nombre;
    }
    public static void main(String[] args) {
        Alumno a1 = new Alumno("Fernando"); //Creando una Instancia
        Alumno a2 = new Alumno("Sophia"); //Creando una Instancia
        System.out.println(a1); 
        System.out.println(a2);
    }
}
