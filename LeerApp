package Aplicacion;

public class LeerApp {
    public static void main(String[] args) {
        Leer moduloLeer = new Leer();
        String texto = moduloLeer.LeerTexto();
System.out.println("Escribiste " + texto);
int numero = moduloLeer.LeerNumeroEntero();
System.out.println("Escribiste el numero " + numero);
}
}
