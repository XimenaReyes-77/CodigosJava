public class ArreglosMultidimensionales {
    public static void main(String[] args) {
        int tamaño = 10;
        for (int i = 1; i <= tamaño; i++) { 
            for (int j = 1; j <= tamaño; j++) {
                System.out.printf("%4d", i * j); 
            }
            System.out.println(); 
        }
    }
}
